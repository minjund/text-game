{
  "name": "nuxt-app",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate && npm run postgenerate",
    "postgenerate": "cp .output/public/200.html .output/public/index.html && cp .output/public/200.html .output/public/404.html",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "deploy": "rm -rf node_modules/.cache/gh-pages && gh-pages -d .output/public"
  },
  "dependencies": {
    "@formkit/auto-animate": "^0.9.0",
    "@vueuse/motion": "^3.0.3",
    "gsap": "^3.13.0",
    "nuxt": "^4.1.3",
    "vue": "^3.5.22",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@nuxtjs/tailwindcss": "^6.14.0",
    "@vite-pwa/nuxt": "^1.1.0",
    "gh-pages": "^6.3.0",
    "terser": "^5.44.1"
  }
}
